                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="1" name="endTime" type="xs:dateTime">
                        <xs:annotation>
                            <xs:documentation>Time when Vod is no longer possible to play.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="publishTime" type="xs:dateTime">
                        <xs:annotation>
                            <xs:documentation>Time when the vod information can be exposed to
                                the customer. Vod can not be played until startTime. If publish
                                is after start or empty (nil), then publishTime = startTime.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:element default="false" maxOccurs="1" minOccurs="1" name="revoked" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>If this flag is set to true then the publication
                            is to be marked for revocation.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element maxOccurs="1" minOccurs="0" name="publicationRights" type="publish:publicationRightsType"/>
