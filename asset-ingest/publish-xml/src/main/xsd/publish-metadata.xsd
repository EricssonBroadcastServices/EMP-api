			<xs:element name="fourGBlocked" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="HDMIBlocked" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="airplayBlocked" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="downloadBlocked" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="streamingBlocked" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="analyticsEnabled" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="sessionShiftEnabled" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="rwEnabled" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="ffEnabled" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="amcDebugLogEnabled" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="locationEnabled" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="minPlayPosition" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="maxPlayPosition" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="jailbrokenBlocked" type="xs:boolean"
