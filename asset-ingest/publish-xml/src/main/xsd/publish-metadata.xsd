		</xs:annotation>
		<xs:sequence>
			<xs:element name="minBitrate" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="maxBitrate" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="maxResWidth" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="maxResHeight" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="playCount" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="maxFileSize" type="xs:long"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="activation" type="xs:dateTime"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="expiration" type="xs:dateTime"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="maxAds" type="xs:integer"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="wifiBlocked" type="xs:boolean"
						minOccurs="0" maxOccurs="1" />
			<xs:element name="threeGBlocked" type="xs:boolean"
